+//        return ((startA + (int) (fraction * (endA - startA))) << 24) |
+//                ((startR + (int) (fraction * (endR - startR))) << 16) |
+//                ((startG + (int) (fraction * (endG - startG))) << 8) |
+//                ((startB + (int) (fraction * (endB - startB))));
