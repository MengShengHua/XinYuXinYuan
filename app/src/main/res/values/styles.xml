<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="home_shape_style">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#A7A8AE</item>
        <item name="android:layout_marginTop">6dp</item>
    </style>

    <style name="home_work_sorbar_enabled_true_style">

        <item name="android:textColor">@color/login_close_c</item>

        <item name="android:textSize">14sp</item>

    </style>



    <style name="login_line_style">

        <item name="android:background">@color/login_line_c</item>

    </style>




</resources>
